{
  "compilerOptions": {
    "lib": [
      "es5",
      "es6"
   ],
    "resolveJsonModule": true,
    "outDir": "dist",
    "module": "commonjs",
    "target": "es5",
    "noImplicitAny": false,
    "moduleResolution": "node",
    "sourceMap": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "types": [
      "koa-body",
    ]
  },
  "exclude": ["./node_modules", "src/**/*.test.ts"]
}
